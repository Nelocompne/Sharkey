{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "entry",
        "corejs": "3.22"
      }
    ],
    "@babel/preset-typescript"
  ],
  "plugins": [
    ["polyfill-corejs3", { "method": "entry-global", "version": "3.20" }]
  ],
  "targets": {
    "esmodules": true
  }
}
